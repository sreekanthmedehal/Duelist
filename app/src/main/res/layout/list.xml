<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
 <TextView
        android:id="@+id/TextView10"
        android:layout_width="140dip"
        android:layout_height="16dp"
        android:layout_margin="0dp"
       
        android:textColor="#07155a"
        android:textSize="12sp" 
        android:text="Id:"/>

    <TextView
        android:id="@+id/TextView1"
        android:layout_width="fill_parent"
        android:layout_height="16dp"
        android:layout_toRightOf="@+id/TextView10"
       
        android:layout_margin="0dp"
        android:textColor="#07155a"
        
        android:textSize="12sp" />
<TextView
        android:id="@+id/TextView11"
        android:layout_width="140dip"
        android:layout_height="16dp"
         android:layout_below="@+id/TextView1"
        android:layout_margin="0dp"
       
         android:textColor="#07155a"
        android:textSize="12sp" 
        android:text="PolicyNo:"/>
    <TextView
        android:id="@+id/TextView2"
        android:layout_width="fill_parent"
        android:layout_height="16dp"
          android:layout_toRightOf="@+id/TextView11"
           android:layout_below="@+id/TextView10"
        android:layout_margin="0dp"
       
         android:textColor="#07155a"
        android:textSize="12sp" />
    
    
    
    <TextView
        android:id="@+id/TextView15"
        android:layout_width="140dip"
        android:layout_height="16dp"
         android:layout_below="@+id/TextView11"
        android:layout_margin="0dp"
        android:textColor="#07155a"
        
        android:textSize="12sp" 
        android:text="DOC:"/>
    <TextView
        android:id="@+id/TextView16"
        android:layout_width="fill_parent"
        android:layout_height="16dp"
        android:layout_toRightOf="@+id/TextView15"
        android:layout_below="@+id/TextView2"
        android:layout_margin="0dp"
        android:textColor="#07155a"
        
        android:textSize="12sp" />

<TextView
        android:id="@+id/TextView12"
        android:layout_width="140dip"
        android:layout_height="16dp"
         android:layout_below="@+id/TextView15"
        android:layout_margin="0dp"
        android:textColor="#07155a"
        
        android:textSize="12sp" 
        android:text="Premium:"/>

    <TextView
        android:id="@+id/TextView3"
        android:layout_width="fill_parent"
        android:layout_height="16dp"
        android:layout_below="@+id/TextView16"
         android:layout_toRightOf="@+id/TextView12"
        android:layout_margin="0dp"
       
         android:textColor="#07155a"
        android:textSize="12sp" />
    <TextView
        android:id="@+id/TextView13a"
        android:layout_width="140dip"
        android:layout_height="16dp"
         android:layout_below="@+id/TextView12"
        android:layout_margin="0dp"
        android:textColor="#07155a"
        
        android:textSize="12sp" 
        android:text="DUE FROM:"/>
    <TextView
       android:id="@+id/TextView4a"
       android:layout_width="fill_parent"
       android:layout_height="16dp"
       android:layout_below="@+id/TextView3"
         android:layout_toRightOf="@+id/TextView13a"
       android:layout_margin="0dp"
       android:textColor="#07155a"
       
       android:textSize="12sp" />
  <TextView
        android:id="@+id/TextView13"
        android:layout_width="140dip"
        android:layout_height="16dp"
         android:layout_below="@+id/TextView13a"
        android:layout_margin="0dp"
        android:textColor="#07155a"
        
        android:textSize="12sp" 
        android:text="DUE TO:"/>
   <TextView
       android:id="@+id/TextView4"
       android:layout_width="fill_parent"
       android:layout_height="16dp"
       android:layout_below="@+id/TextView4a"
         android:layout_toRightOf="@+id/TextView13"
       android:layout_margin="0dp"
       android:textColor="#07155a"
       
       android:textSize="12sp" />
 <TextView
        android:id="@+id/TextView14"
        android:layout_width="140dip"
        android:layout_height="16dp"
         android:layout_below="@+id/TextView13"
        android:layout_margin="0dp"
       
         android:textColor="#07155a"
        android:textSize="12sp" 
        android:text="MODE:"/>
   <TextView
       android:id="@+id/TextView5"
       android:layout_width="fill_parent"
       android:layout_height="16dp"
       android:layout_margin="0dp"
        android:layout_toRightOf="@+id/TextView14"
         android:layout_below="@+id/TextView4"
       android:textColor="#07155a"
       
       android:textSize="12sp" />

     <TextView
         android:id="@+id/TextView6"
         android:layout_width="fill_parent"
         android:layout_height="16dp"
         android:layout_below="@+id/TextView5"
         android:layout_margin="0dp"
        
         android:textColor="#A52A2A"
         android:textSize="12sp" />

      <TextView
          android:id="@+id/TextView7"
          android:layout_width="fill_parent"
          android:layout_height="16dp"
          android:layout_below="@+id/TextView6"
          android:layout_margin="0dp"
         
          android:textColor="#A52A2A"
          android:textSize="12sp" />

      <TextView
          android:id="@+id/TextView8"
          android:layout_width="fill_parent"
          android:layout_height="16dp"
          android:layout_below="@+id/TextView7"
          android:layout_margin="0dp"
         
          android:textColor="#A52A2A"
          android:textSize="12sp" />

      <TextView
          android:id="@+id/TextView9"
          android:layout_width="fill_parent"
          android:layout_height="16dp"
          android:layout_below="@+id/TextView8"
          android:layout_margin="0dp"
         
          android:textColor="#A52A2A"
          android:textSize="12sp" />

      <TextView
          android:id="@+id/TextView20"
          android:layout_width="fill_parent"
          android:layout_height="16dp"
          android:layout_below="@+id/TextView9"
          android:layout_margin="0dp"
          android:layout_toLeftOf="@+id/TextView5"
          android:text="No.of Instalments due : "
          android:textColor="#465a47"
          android:background="#46b1f9"
          android:textSize="12sp" />

       <TextView
           android:id="@+id/TextView19"
           android:layout_width="fill_parent"
           android:layout_height="16dp"
           android:layout_below="@+id/TextView9"
           android:layout_margin="0dp"
           android:layout_toRightOf="@+id/TextView20"
           android:text="0"
             android:background="#46b1f9"
           android:textColor="#465a47"
           android:textSize="12sp" />

</RelativeLayout>